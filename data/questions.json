[
  {
    "id": "qg-legal-scope",
    "order": 1,
    "phase": "Applicability",
    "questions": [
      {
        "id": "q1",
        "text": "Is your organization established in the EU?",
        "type": "yesNo",
        "targetAttribute": "legalScope.isEUEntity"
      },
      {
        "id": "q2",
        "text": "Do you place AI systems on the EU market or use their output in the EU?",
        "type": "yesNo",
        "targetAttribute": "legalScope.placesOnEU"
      },
      {
        "id": "q3",
        "text": "Are you a natural person using AI for non-professional activities?",
        "type": "yesNo",
        "targetAttribute": "legalScope.isNonProfessional"
      }
    ],
    "isComplete": false
  },
  {
    "id": "qg-ai-system-scope",
    "order": 2,
    "phase": "Applicability",
    "questions": [
      {
        "id": "q4",
        "text": "Does the system meet the AI definition?",
        "type": "yesNo",
        "targetAttribute": "aiSystemScope.meetsAIDef"
      },
      {
        "id": "q5",
        "text": "Is it exclusively for military, research, or security purposes?",
        "type": "yesNo",
        "targetAttribute": "aiSystemScope.isExempt"
      },
      {
        "id": "q6",
        "text": "Is it a legacy system without significant changes?",
        "type": "yesNo",
        "targetAttribute": "aiSystemScope.isLegacy"
      }
    ],
    "isComplete": false
  },
