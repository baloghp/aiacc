  {
    "id": "qg-risk-classification",
    "order": 4,
    "phase": "Risk",
    "questions": [
      {
        "id": "q13",
        "text": "Is the system prohibited (e.g., social scoring, real-time biometric ID in public)?",
        "type": "yesNo",
        "targetAttribute": "riskClassification.isProhibited"
      },
      {
        "id": "q14",
        "text": "Does it qualify as high-risk (e.g., in critical infrastructure, employment, law enforcement)?",
        "type": "yesNo",
        "targetAttribute": "riskClassification.isHighRisk"
      },
      {
        "id": "q15",
        "text": "Does it involve limited risk (e.g., chatbots, deep fakes)?",
        "type": "yesNo",
        "targetAttribute": "riskClassification.isLimitedRisk"
      },
      {
        "id": "q16",
        "text": "Is it a general-purpose AI model?",
        "type": "yesNo",
        "targetAttribute": "riskClassification.isGPAI"
      },
      {
        "id": "q17",
        "text": "If Yes: Does it exceed 10^25 FLOPs or pose systemic risks?",
        "type": "yesNo",
        "targetAttribute": "riskClassification.hasSystemicRisk"
      }
    ],
    "isComplete": false
  },
  {
    "id": "qg-gpai-systemic-risk",
